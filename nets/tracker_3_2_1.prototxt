# Ensemble
layer {
  name: "ensemble"
  type: "Eltwise"
  bottom: "3fcs-fc8"
  bottom: "2fcs-fc8_new"
  bottom: "1fcs-fc8_new"
  top: "ensemble-fc8"
  eltwise_param {
    operation: SUM
    coeff: 0.5
    coeff: 0.34
    coeff: 0.16
  }
}