layer {
  name: "concat"
  type: "Concat"
  bottom: "pool5"
  bottom: "pool5_p"
  top: "pool5_concat"
  concat_param {
    axis: 1
  }
}

# 1FCS
layer {
  name: "1fcs-fc7-newb_new"
  type: "InnerProduct"
  bottom: "pool5_concat"
  top: "1fcs-fc7b_new"
  param {
    lr_mult: 10
    decay_mult: 1
  }
  param {
    lr_mult: 20
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "gaussian"
      std: 0.005
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}
layer {
  name: "1fcs-relu7b_new"
  type: "ReLU"
  bottom: "1fcs-fc7b_new"
  top: "1fcs-fc7b_new"
}
layer {
  name: "1fcs-drop7b_new"
  type: "Dropout"
  bottom: "1fcs-fc7b_new"
  top: "1fcs-fc7b_new"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "1fcs-fc8-shapes-new"
  type: "InnerProduct"
  bottom: "1fcs-fc7b_new"
  top: "1fcs-fc8_new"
